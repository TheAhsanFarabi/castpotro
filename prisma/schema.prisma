//
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // Ensure this matches your actual DB (e.g., postgresql, sqlite)
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(cuid())
  email     String     @unique
  name      String?
  age       String?
  password  String
  createdAt DateTime   @default(now())
  updatedAt DateTime   @updatedAt
}